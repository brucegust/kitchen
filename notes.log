SELECT s.AccountID, s.CreatedDate, a.AccountID, t.patientcode, t.encountercode, SUM(t.amount) AS left_to_pay
FROM [PCAR_Data].[dbo].[Statement] s 
INNER JOIN
[PCAR_Data].[dbo].[Account](NOLOCK) a
ON
s.AccountID=a.AccountID
INNER JOIN
[PCAR_Data].[dbo].[txn] (NOLOCK) t
ON 
a.PracticeAccountID= t.accountcode
WHERE s.AccountID=14936650 
AND s.CreatedDate='2016-01-14 03:53:55.180'
AND t.accountcode='381233'
GROUP BY s.AccountID, s.CreatedDate, a.AccountID, t.patientcode, t.encountercode